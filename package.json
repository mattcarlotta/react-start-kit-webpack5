{
  "name": "react-start-kit-webpack5",
  "version": "4.0.0",
  "description": "A starter kit for React and Redux applications.",
  "main": "./src/index.js",
  "repository": "https://github.com/mattcarlotta/react-start-kit-webpack5",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "checkbuild": "es-check es5 './dist/**/*.js'",
    "build": "rimraf dist && NODE_ENV=production webpack --env production",
    "dev": "NODE_ENV=development PORT=3000 webpack serve --no-stats",
    "format": "prettier --write 'src/**/*.js'",
    "lint:js": "eslint 'src/**/*.js' --fix",
    "lint:styles": "stylelint **/*.scss",
    "start": "HOST=http://localhost: NODE_ENV=production PORT=8080 node server.js",
    "test": "NODE_ENV=testing jest --config ./jest.json --passWithNoTests",
    "test:cov": "NODE_ENV=testing jest --config ./jest.json --coverage",
    "test:watch": "NODE_ENV=test jest --config ./jest.json --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run format",
      "npm run lint:js",
      "npm run test"
    ],
    "*.scss": [
      "stylelint **/*.scss"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "alias-dirs": "^0.0.11",
    "axios": "^0.21.1",
    "crypto": "^1.0.1",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "express": "^4.17.1",
    "husky": "^6.0.0",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.23",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "address": "^1.1.2",
    "axios-mock-adapter": "^1.18.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-preset-env": "^1.7.0",
    "browserslist": "^4.12.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "enzyme": "^3.11.0",
    "es-check": "^5.2.3",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-watch": "^7.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^5.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.1",
    "jest-css-modules-transform": "^4.0.0",
    "jest-enzyme": "^7.1.2",
    "jest-haste-map": "^27.0.1",
    "jest-resolve": "^27.0.1",
    "jsdom": "^16.2.2",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-dev-utils": "^11.0.4",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.6.0",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-scss": "^3.17.2",
    "terser-webpack-plugin": "^5.1.2",
    "webpack": "^5.38.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "4.0.0-beta.3",
    "webpack-manifest-plugin": "^3.1.1",
    "webpackbar": "^5.0.0-3"
  }
}
